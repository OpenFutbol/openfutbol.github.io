source 'https://rubygems.org'

# GitHub Pages gem
gem "github-pages", group: :jekyll_plugins

# Additional plugins that work with GitHub Pages
group :jekyll_plugins do
    gem 'jekyll-feed'
    gem 'jekyll-sitemap'
    gem 'jekyll-seo-tag'
end

# Required for Windows
gem "wdm", "~> 0.1.0" if Gem.win_platform?
